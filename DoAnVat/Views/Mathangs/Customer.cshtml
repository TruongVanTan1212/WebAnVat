@{
    ViewData["Title"] = "Thông Tin Khách Hàng";
}

<h2 class="text-center text-info"> Thông Tin Khách hàng </h2>
<br />
<br />
<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            <p>Tên Khách Hàng</p>
        </dt>
        <dd class="col-sm-10">
           @ViewBag.khachhang.Ten 
        </dd>
        <dt class="col-sm-2">
            <p>Số Điện Thoại</p>
        </dt>
        <dd class="col-sm-10">
            @ViewBag.khachhang.DienThoai
        </dd>
        <dt class="col-sm-2">
            <p>Email</p>
        </dt>
        <dd class="col-sm-10">
            @ViewBag.khachhang.Email
        </dd>
    </dl>
</div>
<a asp-action="Signout">Đăng Xuất Nè Fen</a>
